#include<stdio.h>

int main()
{
    int c,n,k;
char a[32];
   scanf("%d",&n);
int m=0;
    for(c=31 ; c>=0 ; c--)
    {
        k = n >> c;
        if(k & 1)
            {
				printf("1");
		a[m++]='1';
		}
        else{
            printf("0");
       a[m++]='0';
		}
	}

printf("\n %s",a);
printf("ok");
    return 0;
}
